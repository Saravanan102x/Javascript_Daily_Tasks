function isPrime(num) {
    if (num <= 1) return false; // 0 and 1 are not prime
    
    for (let i = 2; i * i <= num; i++) { // using i*i instead of Math.sqrt
        if (num % i === 0) return false;
    }
    return true;
}

// Example usage
console.log(isPrime(7));  // true
console.log(isPrime(10)); // false
